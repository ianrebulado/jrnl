<div class="bg-slate-900 text-white min-h-screen flex flex-col p-4 justify-center items-center">
  <h1>Edit category</h1>


<%= form_with model: @category, url: category_path(@category), method: :patch do |form| %>
<div class="flex flex-col w-fit">
  <%= form.label :name %>
  <%= form.text_field :name, class: "text-black" %>
  <%= form.submit :Edit, class: "border bg-green-500 p-3 rounded-md mt-2" %>
  </div>
<% end %>
</div>